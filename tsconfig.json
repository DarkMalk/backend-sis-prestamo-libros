{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "outDir": "./build",
    "removeComments": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
